{
  "expo": {
    "name": "Visara",
    "slug": "visara",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/visara-icon.png",
    "userInterfaceStyle": "light",
    "scheme": "visara",
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Visara to access your photos to identify documents",
          "savePhotosPermission": "Allow Visara to save photos"
        }
      ]
    ],
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/visara-adaptive-icon-black.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.visara.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/visara-adaptive-icon-black.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.visara.app",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/visara-icon.png",
      "bundler": "metro"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "87b53428-5a13-4f53-8dfa-e7eae01f31ef"
      }
    },
    "owner": "amitayks"
  }
}